<cds>
  <!-- Configure your local contest and data folder (used to find or store config files, resources, etc.) -->
  <contest location="C:\icpc\contestData" recordReactions="false">
    <!-- Configure your CCS via REST -->
    <!--
    <ccs url="https://my-ccs" user="" password=""/>
    -->

    <!-- Or configure your older CCS via the XML event feed and optional start time interface -->
    <!--
    <ccs eventFeed="my-ccs:4713" startTime="https://..." user="" password=""/>
    -->

    <!-- Uncomment to enable test (contest playback) mode. Countdown in seconds *or* specific start time, and contest speed multiplier -->
    <!--
    <test countdown="30" startTime="2020-02-19T12:30:00.000-05" multiplier="10.0"/>
    -->
  
    <!-- Uncomment and configure if your contest has team desktop or webcam streaming -->
    <!--
    <video desktop="http://10.1.1.20{0}:9090" webcam="http://10.1.1.20{0}:8080"/>
    -->
  </contest>

  <!-- Administrative domains. Uncomment to set up multiple presentation domains -->
  <!--
  <domain>
    <user name="presAdmin"/>
    <user name="presentation"/>
  </domain>
  -->
</cds>